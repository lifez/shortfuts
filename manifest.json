{
    "manifest_version": 2,
    "name": "shortfuts",
    "description": "Exposes keyboard shortcuts for FIFA 18 FUT web app. Please visit the \"Developer website\" link for the list of shortcuts.",
    "homepage_url": "https://github.com/martellaj/shortfuts",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
       "128": "icon.png"
    },
    "version": "1.0.1",
    "content_scripts": [
        {
            "matches": ["https://www.easports.com/*fifa/ultimate-team/web-app/*", "http://www.easports.com/*fifa/ultimate-team/web-app/*"],
            "js": ["script.js"]
        }
    ],
    "browser_action": {
        "default_icon": {
            "16": "icon.png",
            "24": "icon.png",
            "32": "icon.png"
        }
    }
}
